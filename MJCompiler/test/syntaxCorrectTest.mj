	const bool boolconstant=true;
	
{

int intfunc(int aparm, int bparm) {
return apram+bparm;
}

//sintaksa je ispravna, semantika ne!

	void main() int aint, bint, cint, dint; char chelp; bool boolvar; {
		aniz = new int[deset];
